{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "title": "pet",
    "required": ["petName","dob","type"],
    "additionalProperties": false,
    "properties": {
      "petName": {
        "type": "string"        
      },
      "dob": {
        "type": "string",
        "pattern": "^\\d{4}\\-(0?[1-9]|1[012])\\-(0?[1-9]|[12][0-9]|3[01])"   
      },
      "type": {
        "type": "string"        
      }
    }
  }